{
  "name": "awesome-web",
  "version": "1.0.0",
  "description": "Awesome web",
  "main": "server.js",
  "author": "Vuong Vu <vuongvu333@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "type-check": "tsc",
    "format": "prettier --write **/*.{ts,tsx}",
    "lint": "eslint --ext=jsx,ts,tsx src",
    "lint:fix": "yarn lint --fix",
    "clean": "yarn clean-cache && rimraf .next && rimraf dist",
    "clean-cache": "rimraf node_modules/.cache",
    "predev": "yarn clean",
    "dev": "nodemon",
    "build": "yarn clean && tsc --project tsconfig.server.json && next build",
    "export": "yarn build && next export",
    "prestart": "yarn clean && yarn build",
    "start": "node dist/index.js",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.1.0",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-less": "^1.0.1",
    "antd": "^4.0.1",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.5",
    "date-fns": "^2.8.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "helmet": "^3.21.2",
    "js-cookie": "^2.2.1",
    "less": "^3.10.3",
    "less-vars-to-js": "^1.3.0",
    "lodash": "^4.17.15",
    "lru-cache": "^5.1.1",
    "morgan": "^1.9.1",
    "next": "^9.1.5",
    "next-cookies": "^2.0.3",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^5.0.0",
    "next-size": "^2.1.0",
    "null-loader": "^3.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-injectors": "^1.2.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.37",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/helmet": "^0.0.47",
    "@types/js-cookie": "^2.2.4",
    "@types/lodash": "^4.14.149",
    "@types/lru-cache": "^5.1.0",
    "@types/morgan": "^1.7.37",
    "@types/next-redux-saga": "^3.0.2",
    "@types/next-redux-wrapper": "^3.0.0",
    "@types/node": "^13.5.2",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/redux": "^3.6.0",
    "@types/redux-saga": "^0.10.5",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-redux-saga": "^1.1.3",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.5",
    "nodemon": "^2.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  }
}
