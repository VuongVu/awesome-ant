{
  "name": "awesome-web",
  "version": "1.0.0",
  "description": "Awesome web",
  "main": "server.js",
  "author": "Vuong Vu <vuongvu333@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "type-check": "tsc",
    "format": "prettier --write **/*.{ts,tsx}",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "tslint --fix -p tsconfig.json",
    "clean": "yarn clean-cache && rimraf .next && rimraf dist",
    "clean-cache": "rimraf node_modules/.cache",
    "predev": "yarn clean",
    "dev": "nodemon",
    "build": "yarn clean && tsc --project tsconfig.server.json && next build",
    "export": "yarn build && next export",
    "prestart": "yarn clean && yarn build",
    "start": "node dist/index.js",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-less": "^1.0.1",
    "antd": "^3.25.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.5",
    "date-fns": "^2.7.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "less": "^3.10.3",
    "lru-cache": "^5.1.1",
    "morgan": "^1.9.1",
    "next": "^9.1.3",
    "next-cookies": "^2.0.1",
    "next-redux-saga": "^4.0.2",
    "next-redux-wrapper": "^4.0.1",
    "next-size": "^2.1.0",
    "null-loader": "^3.0.0",
    "numeral": "^2.0.6",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "rc-tween-one": "^2.6.5",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "rimraf": "^3.0.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.37",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/helmet": "^0.0.45",
    "@types/js-cookie": "^2.2.4",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lru-cache": "^5.1.0",
    "@types/morgan": "^1.7.37",
    "@types/next-redux-saga": "^3.0.1",
    "@types/next-redux-wrapper": "^2.0.2",
    "@types/node": "^12.12.7",
    "@types/numeral": "^0.0.26",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/redux": "^3.6.0",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/redux-saga": "^0.10.5",
    "@types/styled-jsx": "^2.2.8",
    "@types/yup": "^0.26.24",
    "babel-plugin-import": "^1.12.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "nodemon": "^1.19.4",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.1",
    "tslint-clean-code": "^0.2.10",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.1.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
