{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier",
    "tslint-clean-code",
    "tslint-consistent-codestyle",
    "tslint-react-hooks"
  ],
  "jsRules": {},
  "rules": {
    "no-console": [
      false
    ],
    "no-empty-interface": false,
    "no-var-requires": false,
    "no-unused-expression": false,
    "ordered-imports": false,
    "interface-name": true,
    "member-ordering": [
      true,
      {
        "order": [
          "public-static-field",
          "public-instance-field",
          "public-constructor",
          "private-static-field",
          "private-instance-field",
          "private-constructor",
          "public-instance-method",
          "protected-instance-method",
          "private-instance-method"
        ],
        "alphabetize": false
      }
    ],
    "object-literal-sort-keys": false,
    "max-line-length": [
      false
    ],
    "semicolon": [
      true,
      "always"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "jsx-no-lambda": false,
    "react-hooks-nesting": {
      "severity": "error",
      "options": {
        "detect-hooks-from-non-react-namespace": true
      }
    },
    "jsdoc-format": false,
    "variable-name": false
  },
  "rulesDirectory": []
}
